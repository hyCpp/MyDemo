Name: AlexaClientSDK
Description: A cross-platform, modular SDK for interacting with the Alexa Voice Service
Version: 1.3.0
Requires: libcurl sqlite3 gstreamer-1.0 gstreamer-app-1.0
prefix=/data/vendor/avs
includedir=${prefix}/include
libdir=${prefix}/lib
Libs: -L${libdir} -lAVSCommon -lACL -lAuthDelegate -lADSL -lAFML -lCertifiedSender -lContextManager -lMediaPlayer -lPlaylistParser -lKWD -lSPHINX -lAIP -lAlerts -lAudioPlayer -lPlaybackController -lSpeakerManager -lSpeechSynthesizer -lSettings -lAVSSystem -lTemplateRuntime -lDefaultClient -lAudioResources -lSampleApp -lSQLiteStorage
Cflags: -I${includedir}
